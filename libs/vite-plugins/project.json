{
  "name": "vite-plugins",
  "targets": {
    "list": {
      "cache": true,
      "executor": "nx:run-commands",
      "options": {
        "command": "bun ../../../apps/one-file/barrel-maker.cli.ts --target=\"./lib/*.js\" --index=index.js --avoid=\".test.\"",
        "cwd": "libs/vite-plugins/src"
      },
      "outputs": [
        "{projectRoot}/src/index.js"
      ]
    },
    "typecheck": {
      "outputs": [
        "{projectRoot}/out-tsc/vitest/**/*.d.ts",
        "{projectRoot}/out-tsc/vitest/tsconfig.tsbuildinfo",
        "{projectRoot}/types/**/*.d.ts",
        "{projectRoot}/tsconfig.lib.tsbuildinfo"
      ]
    }
  }
}