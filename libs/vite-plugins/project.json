{
  "name": "vite-plugins",
  "targets": {
    "list": {
      "cache": true,
      "executor": "nx:run-commands",
      "inputs": ["{workspaceRoot}/libs/utils/src/bin/barrel-maker.cli.ts", "{projectRoot}/src/lib/*.node.js"],
      "options": {
        "command": "bun ../../utils/src/bin/barrel-maker.cli.ts --target=\"./lib/*.js\" --index=index.js",
        "cwd": "{projectRoot}/src"
      },
      "outputs": ["{projectRoot}/src/index.js"]
    },
    "typecheck": {
      "outputs": ["{projectRoot}/out-tsc/vitest/**/*.d.ts", "{projectRoot}/out-tsc/vitest/tsconfig.tsbuildinfo", "{projectRoot}/types/**/*.d.ts", "{projectRoot}/tsconfig.lib.tsbuildinfo"]
    }
  }
}
