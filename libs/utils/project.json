{
  "name": "utils",
  "targets": {
    "list": {
      "cache": true,
      "executor": "nx:run-commands",
      "inputs": ["{projectRoot}/src/**/*.{ts,tsx}", "!{projectRoot}/src/**/*.test.{ts,tsx}"],
      "options": {
        "command": "bun bin/barrel-maker.cli.ts --target=\"./lib/*.ts\" --ext=\".js\"",
        "cwd": "{projectRoot}/src"
      },
      "outputs": ["{projectRoot}/src/index.ts"]
    },
    "ts-check": {}
  }
}
