{
  "name": "utils",
  "targets": {
    "list": {
      "cache": true,
      "executor": "nx:run-commands",
      "options": {
        "command": "bun bin/barrel-maker.cli.ts --target=\"./lib/*.ts\" --avoid=\".test.ts\" --ext=\".js\"",
        "cwd": "libs/utils/src"
      },
      "outputs": ["{projectRoot}/src/index.ts"]
    },
    "typecheck": {
      "outputs": ["{projectRoot}/out-tsc/vitest/**/*.d.ts", "{projectRoot}/out-tsc/vitest/tsconfig.tsbuildinfo"]
    }
  }
}
