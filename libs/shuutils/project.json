{
  "name": "shuutils",
  "targets": {
    "list": {
      "cache": true,
      "executor": "nx:run-commands",
      "options": {
        "command": "tsx src/list.cli.ts",
        "cwd": "libs/shuutils"
      },
      "outputs": [
        "{projectRoot}/src/index.ts"
      ]
    },
    "typecheck": {
      "outputs": [
        "{projectRoot}/out-tsc/vitest/**/*.d.ts",
        "{projectRoot}/out-tsc/vitest/tsconfig.tsbuildinfo"
      ]
    }
  }
}