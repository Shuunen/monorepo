{
  "name": "vet-web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "targets": {
    "test": {
      "options": {
        "coverage.thresholds.100": false
      }
    },
    "ts-check": {
      "command": "tsc -p apps/vet-web/tsconfig.app.json --noEmit",
      "options": {
        "cwd": "."
      }
    },
    "typecheck-spec": {
      "command": "tsc -p apps/vet-web/tsconfig.spec.json --noEmit",
      "options": {
        "cwd": "."
      }
    },
    "typecheck": {
      "dependsOn": ["ts-check"],
      "executor": "nx:noop" 
    }
  }
}