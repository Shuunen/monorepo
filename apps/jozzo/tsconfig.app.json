{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "outDir": "out-tsc/tsc",
    "paths": {
      "react": ["./node_modules/preact/compat/"],
      "react-dom": ["./node_modules/preact/compat/"]
    },
    "rootDir": "src",
    "tsBuildInfoFile": "out-tsc/tsc/tsconfig.app.tsbuildinfo",
    "types": ["node", "@nx/react/typings/cssmodule.d.ts", "@nx/react/typings/image.d.ts", "vite/client", "vite-plugin-svgr/client"]
  },
  "exclude": ["out-tsc", "dist", "src/**/*.test.ts", "src/**/*.test.tsx", "vite.config.ts"],
  "extends": "../../tsconfig.base.json",
  "include": ["src/**/*.js", "src/**/*.jsx", "src/**/*.ts", "src/**/*.tsx"],
  "references": [
    {
      "path": "../../libs/vite-plugins/tsconfig.lib.json"
    },
    {
      "path": "../../libs/utils/tsconfig.lib.json"
    }
  ]
}
